@model gongshangchaxun.Models.setupdb
@{
    ViewBag.Title = "setupcanshu";
    Layout = "~/Views/Shared/_Layoutgl.cshtml";
}
<div style="border-bottom:solid 1px #b5cbe3; margin-top:30px;margin-bottom:20px;">
         <div style=" float:left; padding-left:5px;font-size:18px; ">系统管理员——参数管理：</div>
         <div style="float:right;">
          <a href="/test/"><img src="/content/images/back.png" style="float:right; margin-top:1px; margin-bottom:2px; margin-right:50px; cursor: pointer;border-width:0px 0px 0px 0px;height:30px;"/></a>
         </div>
         <div class="clr"></div>
     </div>
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>参数创建表单</legend>
        
         <table style="margin:0 auto; font-size:20px; color:black;padding:20px">
            <tr>
                <td>   参数</td>
                <td> @Html.EditorFor(model => model.canshu)
            @Html.ValidationMessageFor(model => model.canshu)</td>
            </tr>
            <tr></tr>
             <tr>
                <td> 赋值</td>
                <td>     @Html.EditorFor(model => model.zhi)
            @Html.ValidationMessageFor(model => model.zhi)</td>
            </tr>
            <tr></tr>
           
            <tr>
                <td colspan="2" style="text-align:center; "> <input type="submit" value="创建" style="font-size:20px; color:black;" /></td>
            </tr>

        </table>
     
    </fieldset>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
