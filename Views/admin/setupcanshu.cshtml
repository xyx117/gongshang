@model IEnumerable<gongshangchaxun.Models.setupdb>

@{
    ViewBag.Title = "setupcanshu";
    Layout = "~/Views/Shared/_Layoutgl.cshtml";
}

@*<h3>企业信用查询系统——系统管理员操作：</h3>*@


<div style="border-bottom:solid 1px #b5cbe3; margin-top:30px;">
         <div style=" float:left; padding-left:5px;font-size:18px; ">系统管理员——参数管理：</div>
         <div style="float:right;">
          <a href="/admin/"><img src="/content/images/fanhui.png" style="float:right; margin-top:1px; margin-bottom:2px; margin-right:50px; cursor: pointer;border-width:0px 0px 0px 0px;height:30px;"/></a>
         </div>
         <div class="clr"></div>
     </div>

@*<div style="color:red;">@ViewBag.error</div>*@

<fieldset>
        <legend>设置表单</legend>
     
        @Html.ActionLink("创建新的参数", "Create","admin")
         
 </fieldset>



    <fieldset style="padding-top:20px;">
        <legend>已设置的参数</legend>


        <table class="tb3">
         <tr>
           <th style="width:20%;" >序号</th>
           <th style="width:40%;" >参数</th>
           <th style="width:40%;">赋值</th>
                 
        </tr>
         <tbody id="abnormalListBody">
            @{int xuhao = 0; }
            
            @foreach (var item in Model) {
              <tr>
                  <td>
                     @{ xuhao=xuhao+1;
                        @xuhao; }
                 </td>
                 <td>
                     @Html.DisplayFor(ModelItem =>item.canshu)
                 </td>
                 <td>
                     @Html.DisplayFor(ModelItem =>item.zhi)
                              
                 </td>
                       
                 </tr>
             }
            </tbody>

        </table>
    </fieldset>
    